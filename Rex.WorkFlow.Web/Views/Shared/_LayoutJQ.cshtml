@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,Chrome=1" />
    <meta http-equiv="X-UA-Compatible" content="IE=9" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
    <title>@ViewData["Title"]</title>
    <link href="/ui/css/bootstrap.min14ed.css" asp-append-version="true" rel="stylesheet" />
    <link href="/ui/css/font-awesome.css" asp-append-version="true" rel="stylesheet">
    <link href="/ui/css/animate.min.css" asp-append-version="true" rel="stylesheet">
    <link href="/ui/js/plugins/layer/skin/layer.css" asp-append-version="true" rel="stylesheet" />
    <link href="/ui/css/plugins/toastr/toastr.min.css" asp-append-version="true" rel="stylesheet" />
    <link href="/ui/css/plugins/iCheck/custom.css" asp-append-version="true" rel="stylesheet" />
    <link href="/ui/css/style.css" asp-append-version="true" rel="stylesheet">
    <link href="/ui/css/common.css" asp-append-version="true" rel="stylesheet" />
    @RenderSection("css", false)
</head>
<body class="fixed-sidebar full-height-layout skin-1 pace-done gray-bg">
    @RenderBody()
    <script src="/ui/js/jquery.min.js"></script>
    <script src="/ui/js/plugins/polyfill/polyfill.min.js"></script>
    <script src="/ui/js/bootstrap.min.js"></script>
    <script src="/ui/js/plugins/pace/pace.min.js"></script>
    <script src="/ui/js/axios.min.js"></script>
    <script src="/ui/js/plugins/iCheck/icheck.min.js"></script>
    <script src="/ui/js/plugins/validate/jquery.validate.min.js"></script>
    <script src="/ui/js/plugins/validate/messages_zh.min.js"></script>
    <script src="/ui/js/plugins/layer/layer.min.js"></script>
    <script src="/ui/css/plugins/print/jQuery.print.min.js"></script>
    <script src="/ui/js/jquery.pager.js" asp-append-version="true"></script>
    <script src="/ui/js/jquery-extension.js" asp-append-version="true"></script>
    <script src="/ui/js/jquery.utils.js" asp-append-version="true"></script>
    @RenderSection("scripts", false)
</body>
</html>